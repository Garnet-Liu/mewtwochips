{
  "name": "next-learn-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 4000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write src/**/*.{ts,tsx,css}",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'",
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@auth/firebase-adapter": "^1.0.9",
    "@livekit/components-react": "^1.4.2",
    "@livekit/components-styles": "^1.0.7",
    "@radix-ui/themes": "^2.0.2",
    "@t3-oss/env-nextjs": "^0.7.1",
    "axios": "^1.6.2",
    "class-variance-authority": "^0.7.0",
    "firebase": "^10.7.1",
    "firebase-admin": "^11.11.1",
    "https-proxy-agent": "^7.0.2",
    "livekit-client": "^1.15.4",
    "livekit-server-sdk": "^1.2.7",
    "luxon": "^3.4.4",
    "next": "14.0.4",
    "next-auth": "^4.24.5",
    "pokenode-ts": "^1.20.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.49.2",
    "stream-chat": "^8.14.4",
    "stream-chat-react": "^11.1.1",
    "swr": "^2.2.4",
    "tailwind-merge": "^2.1.0",
    "tailwindcss-animate": "^1.0.7",
    "viem": "^1.19.13",
    "wagmi": "^1.4.11",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/luxon": "^3.3.7",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "postcss": "^8",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
