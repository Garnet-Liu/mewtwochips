  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "prepare": "husky",
    "postinstall": "patch-package",
    "codegen": "graphql-codegen --config codegen.ts",
    "extract-translations": "npx i18next -c i18next-parser.config.ts",
